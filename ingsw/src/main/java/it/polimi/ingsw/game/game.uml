         case HUMAN:              return new Sector(HUMAN, false);&#xD;
         case NOT_VALID:          return new Sector(NOT_VALID, false);&#xD;
         default:                 throw new SectorException(&quot;Illegal sector code&quot;);&#xD;
     }&#xD;
 </body>
